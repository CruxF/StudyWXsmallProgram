page {
  background-color: #369;
  color: white;
  font-family: '黑体','宋体';
}
.title {
  color: white;
  font-size: 20px;
  text-align: center;
  line-height: 30px;
  background-color: orange;
  box-shadow: 2px 2px 10px #B8B8B8;
  margin-bottom: 10px;
}
.pull-right {
  float: right;
}
.pull-left {
  float: left;
}
.time {
  height: 330rpx;
}
.big-circle {
  width: 298rpx;
  height: 298rpx;
  position: absolute;
  border-radius: 50%;
}
.right {
  /* 裁剪图形 */
  clip: rect(0, auto, auto, 149rpx);
  background-color: black;
}
.blue {
  background-color: #9cf;
}
.blue1 {
  background-color: #09f;
}
.btnGroup {
  display: flex;
  flex-direction: row;
}
.btn {
  background-color: #09c;
  color: white;
}
.white {
  background-color: white;
}
.circle {
  width: 258rpx;
  height: 258rpx;
  position: absolute;
  border-radius: 50%;
  margin-left: 21rpx;
  margin-top: 21rpx;
}
.first {
  left: 5%;
  text-align: center;
  color: black;
}
.first text{
  margin-top: 108rpx;
  display: block;
}
.second {
  left: 55%;
  text-align: center;
  color: black;
}
.second text {
  margin-top: 108rpx;
  display: block;
}
.desc {
  padding: 20rpx;
}